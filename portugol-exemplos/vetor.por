algoritmo
 	declare contador, vetor[5] numerico
	vetor[1] <- 1
	vetor[2] <- 10
	vetor[3] <- 11
	vetor[4] <- 12
	vetor[5] <- 13
	
 	escreva vetor[1]
 	escreva vetor[2]
 	escreva vetor[3]
 	escreva vetor[4]
 	escreva vetor[5]
 	
	escreva "Leitura por laço"
 	para contador <- 1 ate 5 faca
 	inicio
 		leia vetor[contador]
 		escreva vetor[contador]
 	fim
fim_algoritmo
